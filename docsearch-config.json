{
  "index_name": "firefish-docs",
  "start_urls": [
    "https://anananas42.github.io/docs-Firefish/docs/"
  ],
  "sitemap_urls": [
    "https://anananas42.github.io/docs-Firefish/sitemap.xml"
  ],
  "sitemap_alternate_links": true,
  "stop_urls": [],
  "selectors": {
    "lvl0": {
      "selector": "[class*='sidebar'] [class*='menu__link--active']:first-of-type",
      "default_value": "Documentation"
    },
    "lvl1": "[class*='docMainContainer'] h1",
    "lvl2": "[class*='docMainContainer'] h2",
    "lvl3": "[class*='docMainContainer'] h3",
    "lvl4": "[class*='docMainContainer'] h4",
    "lvl5": "[class*='docMainContainer'] h5",
    "lvl6": "[class*='docMainContainer'] h6",
    "text": "[class*='docMainContainer'] p, [class*='docMainContainer'] li, [class*='docMainContainer'] td"
  },
  "selectors_exclude": [
    ".hash-link",
    ".anchor",
    ".table-of-contents"
  ],
  "custom_settings": {
    "separatorsToIndex": "_",
    "attributesForFaceting": [
      "language",
      "version",
      "type"
    ],
    "attributesToRetrieve": [
      "hierarchy",
      "content",
      "anchor",
      "url",
      "url_without_anchor",
      "type"
    ]
  },
  "js_render": true,
  "use_anchors": true,
  "strip_chars": " .,;:#"
}